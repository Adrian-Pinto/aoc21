{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "rules": {
      "no-console": 0,
      "no-extend-native": 0,
      "import/extensions": [0, {
        "js": "never"
      }]
    }
}
